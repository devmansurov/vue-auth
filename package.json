{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port=4000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "4.1.1",
    "@websanova/vue-auth": "^4.1.0",
    "axios": "^0.21.1",
    "core-js": "3.6.5",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.11",
    "vue-axios": "2.1.5",
    "vue-resource": "1.5.1",
    "vue-router": "3.4.3",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-service": "4.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.6.11"
